<apex:page standardController="Registration__c">
    <apex:form >
        <apex:pageBlock title="Registration Detail">
            
            <apex:pageBlockSection >
                <apex:outputField value="{! Registration__c.Id }" />
                <apex:outputField value="{! Registration__c.Name }" />
                <apex:outputField value="{! Registration__c.ANNUAL_INCOME__c}"  />
                <apex:outputField value="{! Registration__c.LastmodifiedById }" />
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton value="View" action="{!URLFOR($Action.Registration__c.View, Registration__c.Id)}"/>
                <apex:commandButton value="Edit" action="{!URLFOR($Action.Registration__c.Edit, Registration__c.Id)}"/>
                <apex:commandButton value="Delete" action="{!URLFOR($Action.Registration__c.Delete, Registration__c.Id)}"/> 
                 <apex:commandButton value="Clone" action="{!URLFOR($Action.Registration__c.Clone,Registration__c.Id)}"/>
            </apex:pageBlockButtons>
            
        </apex:pageBlock>
        
    </apex:form>
</apex:page>